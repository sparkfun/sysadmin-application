#-------------------------------------------------------------------------------------------------------------
#-------------------------------------------------------------------------------------------------------------
#-------------------------------------------------------------------------------------------------------------
#   Config for pfsense virt.
#-------------------------------------------------------------------------------------------------------------
#-------------------------------------------------------------------------------------------------------------
#-------------------------------------------------------------------------------------------------------------
auto eth3
iface eth3 inet manual

# local lan vlan
auto eth3.2
iface eth3.2 inet manual

# Home wireless vlan
auto eth3.3
iface eth3.3 inet manual

# ppoe vlan
auto eth3.4
iface eth3.4 inet manual

# lab vlan
auto eth3.666
iface eth3.666 inet manual

auto br2pf
iface br2pf inet manual
        bridge_ports eth3.2
        bridge_stp off
        bridge_fd 0
        bridge_maxwait 0


auto br3pf
iface br3pf inet manual
        bridge_ports eth3.3
        bridge_stp off
        bridge_fd 0
        bridge_maxwait 0

auto br4pf
iface br4pf inet manual
        bridge_ports eth3.4
        bridge_stp off
        bridge_fd 0
        bridge_maxwait 0


auto br666pf
iface br666pf inet manual
        bridge_ports eth3.666
        bridge_stp off
        bridge_fd 0
        bridge_maxwait 0
